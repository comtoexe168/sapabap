DATA: GF_REF TYPE REF TO DATA.
FIELD-SYMBOLS : <GF_FS> TYPE TABLE,
                                <GF_FS1> TYPE ANY.
PARAMETER: PA_TABLE LIKE DD03L-TABNAME OBLIGATORY.
CREATE DATA GF_REF TYPE TABLE OF (PA_TABLE).
ASSIGN GF_REF->* TO <GF_FS>.
SELECT * FROM (PA_TABLE) INTO CORRESPONDING FIELDS OF TABLE <GF_FS>.
LOOP AT <GF_FS> ASSIGNING <GF_FS1>.
  WRITE: / <GF_FS1>.
ENDLOOP.
